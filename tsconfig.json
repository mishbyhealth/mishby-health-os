{
  "compilerOptions": {
    "target": "ES2020",
    "lib": ["ES2020", "DOM", "DOM.Iterable"],
    "jsx": "react-jsx",
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "strict": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowJs": false,
    "types": ["vite/client"],
    "baseUrl": ".",
    "paths": {
      "@/*": ["src/*"],
      "mho/*": ["mho/*"]
    }
  },
  "include": ["src", "mho", "core", "vite-env.d.ts"],
  "exclude": [
  "node_modules",
  "dist",
  "features",
  "**/*.test.*",
  "**/__tests__/**",
  "src/_archive",
  "public/_mho_trial_archive",
  "src/pages/__backup_*",
  "**/*.bak"
]

}
